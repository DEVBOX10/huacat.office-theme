    {
      "name": "Markdown heading",
      "scope": ["markup.heading", "markup.heading entity.name"],
      "settings": {
        "foreground": "#4472c4"
      }
    },
    {
      "name": "Markdown list",
      "scope": ["markup.list punctuation.definition.list.begin"],
      "settings": {
        "foreground": "#f4b183"
      }
    },
    {
      "name": "Markdown link",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#8064a2"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#70ad47",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#70ad47",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown italic bold",
      "scope": ["markup.italic markup.bold", "markup.bold markup.italic"],
      "settings": {
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["text.html.markdown markup.inline.raw"],
      "settings": {
        "foreground": "#5b9bd5"
      }
    },
    {
      "name": "Markdown quote",
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#ffc000",
